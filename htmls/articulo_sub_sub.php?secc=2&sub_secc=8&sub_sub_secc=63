jQuery(document).ready(function(){
    $('map > area.fancybox').click(function(e) {
        e.preventDefault();
        var url = $(this).attr('href');
        var title = $(this).attr('title');
        var type = $(this).attr('rel');
        $.fancybox({
            'title': title,
            'titlePosition': 'inside',
            'href' : url,
            'type' : type
        });
    });
<!-- fancy box -->
